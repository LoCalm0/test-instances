CREATE TABLE IM_BS_MODEL_TABLE
(
    ID           NUMBER(20) primary key,
    TABLE_NAME   VARCHAR2(40),
    TABLE_REMARK VARCHAR2(40),
    CREATE_USER  NUMBER(20),
    CREATE_TIME  NUMBER(20),
    UPDATE_USER  NUMBER(20),
    UPDATE_TIME  NUMBER(20),
    CREATE_DEPT  NUMBER(20),
    TENANT_ID    VARCHAR2(20),
    STATUS       NUMBER(1),
    IS_DELETED   NUMBER(1)
);

COMMENT ON TABLE IM_BS_MODEL_TABLE               IS '模型表';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.ID           IS '主键';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.TABLE_NAME   IS '表名';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.TABLE_REMARK IS '表备注';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.CREATE_USER  IS '创建用户';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.CREATE_TIME  IS '创建时间';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.UPDATE_USER  IS '更新用户';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.UPDATE_TIME  IS '更新时间';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.CREATE_DEPT  IS '创建部门';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.TENANT_ID    IS '租户id';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.STATUS       IS '状态';
COMMENT ON COLUMN IM_BS_MODEL_TABLE.IS_DELETED   IS '是否已删除';


CREATE TABLE IM_BS_MODEL_TABLE_FIELDS
(
    ID                            NUMBER(20) primary key,
    MODEL_TABLE_ID                NUMBER(20),
    FIELD_NAME                    VARCHAR2(40),
    FIELD_REMARK                  VARCHAR2(40),
    FIELD_TYPE                    VARCHAR2(20),
    CREATE_USER                   NUMBER(20),
    CREATE_TIME                   NUMBER(20),
    UPDATE_USER                   NUMBER(20),
    UPDATE_TIME                   NUMBER(20),
    CREATE_DEPT                   NUMBER(20),
    TENANT_ID                     VARCHAR2(20),
    STATUS                        NUMBER(1),
    IS_DELETED                    NUMBER(1)
);

COMMENT ON TABLE IM_BS_MODEL_TABLE_FIELDS                  IS '模型表字段';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.ID              IS '主键';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.MODEL_TABLE_ID  IS '模型表id';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.FIELD_NAME      IS '字段名';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.FIELD_REMARK    IS '字段备注';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.FIELD_TYPE      IS '字段类型';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.CREATE_USER     IS '创建用户';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.CREATE_TIME     IS '创建时间';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.UPDATE_USER     IS '更新用户';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.UPDATE_TIME     IS '更新时间';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.CREATE_DEPT     IS '创建部门';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.TENANT_ID       IS '租户id';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.STATUS          IS '状态';
COMMENT ON COLUMN IM_BS_MODEL_TABLE_FIELDS.IS_DELETED      IS '是否已删除';